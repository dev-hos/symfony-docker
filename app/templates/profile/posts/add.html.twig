{% extends 'base.html.twig' %}

{% block title %}Ajout | Articles
{% endblock %}

{% block body %}
	<h1>Ajouter un article</h1>
	{{ form_start(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.content) }}
        {{ form_row(form.categories) }}
        {{ form_row(form.keywords) }}
        {{ form_row(form.featuredImage) }}
        <button type="submit">Envoyer</button>
	{{ form_end(form) }}

{% endblock %}
